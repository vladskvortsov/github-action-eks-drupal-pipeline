
module "s3_bucket" {
  source = "terraform-aws-modules/s3-bucket/aws"

  bucket = "drupal-eks-bucket"
  acl    = "private"

  control_object_ownership = true
  object_ownership         = "ObjectWriter"

}


terraform {
  backend "s3" {
    bucket = "drupal-eks-bucket"
    region = "eu-west-2"
    key = "terraform.tfstate"
  }
}